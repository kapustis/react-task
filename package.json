    "react-hot-loader": "^4.12.21",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "next",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.5.5",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "babel-core": "^6.26.3",
