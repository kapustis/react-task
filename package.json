{
  "name": "react-test-task",
  "version": "1.0.0",
  "description": "A boilerplate for playing around with react. Developed to be given as a test task for internship.",
  "main": "index.js",
  "scripts": {
    "test_watch": "mocha --opts mocha.opts --watch app/**/*spec.js",
    "start": "node server",
    "build": "rimraf dist && webpack --config ./webpack.production.config.js --progress --profile --colors",
    "dev": "concurrently \"npm run start\" \"npm run s-service\"",
    "s-service": "npm run start --prefix streaming-service",
    "eslint": "eslint ."
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "es6",
    "react",
    "redux"
  ],
  "author": "Oleh Meleshko",
  "license": "MIT",
